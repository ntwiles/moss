let secret_number = 68;

print_line("I've thought of a secret number. Try to guess it!");

loop {
    let guess = int(read_line());

    if guess > secret_number {
        print_line("Too high!");
    } elif guess < secret_number {
        print_line("Too Low!");
    } else {
        print_line("You got it!");
        break;
    };
};

print_line("Game over.");
