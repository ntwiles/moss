let a = |k: Int, x1: Func, x2: Func, x3: Func, x4: Func, x5: Func|: Int => {
    let b = ||: Int => {
        a(k - 1, b, x1, x2, x3, x4);
    };

    if k <= 0 {
        x4() + x5();
    } else {
        b();
    };
};

let result = a(
    10,
    ||: Int => { 1; },
    ||: Int => { -1; },
    ||: Int => { -1; },
    ||: Int => { 1; },
    ||: Int => { 0; }
);

print_line(result);